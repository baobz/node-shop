{{extend './layout.art'}}

{{block 'content'}}
<form action="" method="POST">
  <div>
    <label>姓名:</label>
    <input class="username" type="text" name="username">
  </div>
  <div>
    <label>密码:</label>
    <input class="pwd" type="password" name="pwd">
  </div>
  <div>
    <label>图片上传</label>
    <input type="file" multiple>
  </div>
  <button class="btn" type="submit">提交</button>
</form>

{{/block}}